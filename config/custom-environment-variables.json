{
  "openapiConfig": {
    "filePath": "OPENAPI_FILE_PATH"
  },
  "telemetry": {
    "serviceName": "TELEMETRY_SERVICE_NAME",
    "hostname": "TELEMETRY_HOST_NAME",
    "version": "TELEMETRY_SERVICE_VERSION",
    "logger": {
      "level": "LOG_LEVEL",
      "prettyPrint": {
        "__name": "LOG_PRETTY_PRINT_ENABLED",
        "__format": "boolean"
      }
    },
    "tracing": {
      "enabled": "TELEMETRY_TRACING_ENABLED",
      "url": "TELEMETRY_TRACING_URL"
    },
    "metrics": {
      "enabled": "TELEMETRY_METRICS_ENABLED",
      "url": "TELEMETRY_METRICS_URL",
      "interval": "TELEMETRY_METRICS_INTERVAL"
    }
  },
  "server": {
    "port": "SERVER_PORT",
    "request": {
      "payload": {
        "limit": "REQUEST_PAYLOAD_LIMIT"
      }
    },
    "response": {
      "compression": {
        "enabled": {
          "__name": "RESPONSE_COMPRESSION_ENABLED",
          "__format": "boolean"
        }
      }
    }
  },
  "db": {
    "host": "DB_HOST",
    "port": "DB_PORT",
    "username": "DB_USERNAME",
    "password": "DB_PASSWORD",
    "database": "DB_NAME",
    "schema": "DB_SCHEMA",
    "ssl": {
      "enabled": {
        "__name": "DB_ENABLE_SSL_AUTH",
        "__format": "boolean"
      },
      "ca": "DB_CA_PATH",
      "key": "DB_KEY_PATH",
      "cert": "DB_CERT_PATH"
    }
  },
  "staleTasksSweeperCron": {
    "enabled": {
      "__name": "STALE_TASKS_SWEEPER_CRON_ENABLED",
      "__format": "boolean"
    },
    "schedule": "STALE_TASKS_SWEEPER_CRON_SCHEDULE"
  },
  "task": {
    "staleTaskThresholdInMinutes": {
      "__name": "TASK_STALE_THRESHOLD_IN_MINUTES",
      "__format": "integer"
    }
  }
}
